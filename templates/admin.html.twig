<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
        <!-- CSS only -->
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
            {# {{ encore_entry_link_tags('app') }} #}
            <link rel="stylesheet" href="{{asset('css/admin.css')}}">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
        {% endblock %}

        {% block javascripts %}
            <!-- JavaScript Bundle with Popper -->
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
            {# {{ encore_entry_script_tags('app') }} #}
        {% endblock %}
    </head>
    <body>
    
        <div class="container">
            <div class="row">
                <div class="col-2">
                    <h2>Dashboard</h2>
                    <div class="list-group">
                        <a href="{{ path('admin_user_index') }}" class="list-group-item list-group-item-action}}" aria-current="true">users</a>
                        <a href="{{ path('admin_collections_index') }}" class="list-group-item list-group-item-action}}" aria-current="true">collections</a>
                        <a href="{{ path('admin_dashboard') }}" class="list-group-item list-group-item-action}}" aria-current="true">dashboard</a>
                    </div>
                </div>

		    	<div class="col-10">
                     {% block body %}  {% endblock %}
				{# {% block content %}{% endblock %} #}
			    </div>
		    </div>
	    </div>

  
    </body>
</html>